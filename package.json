{
  "version": "1.1.7",
  "title": "woocommerce-related-accessories",
  "description": "Displays a list of related accessories on product single page.",
  "scripts": {
    "build": "gulp build --concat false",
    "build:production": "gulp build:production --concat false"
  },
  "gulpPaths": {
    "destDir": "./dist",
    "scripts": {
      "srcDir": "./assets/scripts",
      "src": [
        "./assets/scripts/**/*.js"
      ],
      "dest": "./dist/scripts"
    },
    "styles": {
      "srcDir": "./assets/styles",
      "src": [
        "./assets/styles/**/*.scss"
      ],
      "dest": "./dist/styles"
    }
  },
  "devDependencies": {
    "@netzstrategen/eslint-config": "^1.0.1",
    "@netzstrategen/gulp-task-collection": "^3.1.1",
    "@netzstrategen/stylelint-config": "^2.1.0"
  }
}
