{
  "version": "1.1.8",
  "title": "woocommerce-related-accessories",
  "description": "Displays a list of related accessories on product single page.",
  "dependencies": {
    "@netzstrategen/gulp-task-collection": "^4.1.3"
  },
  "gulpPaths": {
    "destDir": "./dist",
    "images": {
      "src": "./assets/img/**/*.{jpg,jpeg,png,gif,svg}",
      "dest": "./dist/img"
    },
    "scripts": {
      "srcDir": "./assets/scripts",
      "src": [
        "./assets/scripts/**/*.js"
      ],
      "dest": "./dist/scripts"
    },
    "styles": {
      "srcDir": "./assets/styles",
      "src": [
        "./assets/styles/**/*.scss"
      ],
      "dest": "./dist/styles"
    },
    "templates": "./templates/**/*.twig"
  },
  "devDependencies": {
    "babel-loader": "^9.1.2",
    "eslint-config-google": "^0.13.0"
  }
}
